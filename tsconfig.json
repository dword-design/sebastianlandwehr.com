{
  "compilerOptions": {
    "declaration": false,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  },
  "exclude": [
    "test-results"
  ],
  "extends": "./.nuxt/tsconfig.json",
  "vueCompilerOptions": {
    "cssModulesLocalsConvention": "camelCaseOnly",
    "strictCssModules": true
  }
}
